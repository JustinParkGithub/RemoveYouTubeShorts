{
    "manifest_version": 3,
    "name": "Remove YouTube Shorts",
    "version": "1.2.0",
    
    "content_scripts": [
        {
            "js": ["remove.js"],
            "matches": ["*://www.youtube.com/*"]
        }
    ],
    "icons": {
        "16": "assests/16.png",
        "32": "assests/32.png",
        "48": "assests/48.png",
        "128": "assests/128.png"
    },
    "description": "A Chrome Extension that removes YouTube Shorts from the YouTube website.",
    "author": "Justin Park"
}